{
  "name": "huzma",
  "version": "0.2.3",
  "license": "MIT",
  "type": "module",
  "description": "A manifest format for browser-based package managers",
  "scripts": {
    "test": "vitest run",
    "test-watch": "vitest watch",
    "ci": "npm run test",
    "docs": "typedoc --out docs src/index.ts",
    "build-node": "tsc --project tsconfig.json",
    "build-web": "node ./webBuild.mjs",
    "build": "npm run build-node && npm run build-web"
  },
  "dependencies": {
    "small-semver": "^0.1.0"
  },
  "devDependencies": {
    "esbuild": "^0.17.10",
    "typedoc": "^0.23.25",
    "typescript": "^4.9.5",
    "vitest": "^0.28.5"
  },
  "keywords": [
    "manifest",
    "browser",
    "dependencies"
  ],
  "homepage": "https://github.com/moomoolive/huzma",
  "bugs": {
    "url": "https://github.com/moomoolive/huzma/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/moomoolive/huzma"
  },
  "main": "dist/index.js",
  "files": [
    "dist/**/*"
  ]
}
